# Firebase Web SDK config (Create React App requires REACT_APP_ prefix)
REACT_APP_FIREBASE_API_KEY=
REACT_APP_FIREBASE_AUTH_DOMAIN=
REACT_APP_FIREBASE_PROJECT_ID=
REACT_APP_FIREBASE_STORAGE_BUCKET=
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=
REACT_APP_FIREBASE_APP_ID=
REACT_APP_FIREBASE_MEASUREMENT_ID=

# Opcional: for√ßa Firestore long-polling (ajuda quando fica "offline" / requests pendentes)
# REACT_APP_FIRESTORE_FORCE_LONG_POLLING=1

# Admin allow-list (optional)
# Either set exact emails (comma-separated) OR a domain.
REACT_APP_ADMIN_EMAILS=admin@exemplo.com,outro@exemplo.com
REACT_APP_ADMIN_EMAIL_DOMAIN=
